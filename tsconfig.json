{
    "compilerOptions": {
      "target": "es6",
      "module": "commonjs",
      "moduleResolution": "node",
      "noResolve": false,
      "noImplicitAny": false,
      "removeComments": true,
      "sourceMap": true,
      "allowJs": true,
      "jsx": "react-jsx",
      
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules", "**/*.spec.ts"]
  }